CREATE TABLE testicsv (postcode varchar(50) NOT NULL,year varchar(50) NOT NULL,operator varchar(50) NOT NULL,operatorID varchar(50),downloadMeasurements varchar(50),averageDownload varchar(50),topDownload varchar(50),uploadMeasurements varchar(50),averageUpload varchar(50),topUpload varchar(50),PRIMARY KEY(postcode, year, operator));



